{
  "expo": {
    "name": "TourneyApp-MEU",
    "slug": "TourneyApp-MEU",
    "scheme": "myapp",
    "version": "1.0.0",
    "owner": "esmdev",
    "orientation": "portrait",
    "icon": "./assets/images/logo-onwhite.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/player.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.esmdev.TourneyAppMEU"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-onwhite.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.esmdev.TourneyAppMEU",
      "googleServicesFile": "./google-services.json",
      "permissions": ["INTERNET"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "expo-updates"
    ],
    "extra": {
      "router": {
        "origin": false
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD",
      "url": "https://u.expo.dev/489fb70f-4a80-4dec-9c24-be48db502895"
    }
  }
}